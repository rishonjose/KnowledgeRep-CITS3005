<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/git-onto-logic"
         xmlns="http://example.org/git-onto-logic#">

<owl:Ontology rdf:about="http://example.org/git-onto-logic"/>

<owl:ObjectProperty rdf:about="#hasBranch">
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:range rdf:resource="#Branch"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#containsFile">
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:range rdf:resource="#File"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#initialCommit">
  <rdfs:domain rdf:resource="#Branch"/>
  <rdfs:range rdf:resource="#Commit"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#onBranch">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#Branch"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#authoredBy">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#User"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#updatesFile">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#File"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#parent">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#Commit"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasName">
  <rdfs:domain rdf:resource="#User"/>
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:domain rdf:resource="#Branch"/>
  <rdfs:domain rdf:resource="#File"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#message">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#timestamp">
  <rdfs:domain rdf:resource="#Activity"/>
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Repository">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasBranch"/>
      <owl:onClass rdf:resource="#Branch"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Branch">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#initialCommit"/>
      <owl:onClass rdf:resource="#Commit"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Commit">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#authoredBy"/>
      <owl:onClass rdf:resource="#User"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#onBranch"/>
      <owl:onClass rdf:resource="#Branch"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#timestamp"/>
      <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#updatesFile"/>
      <owl:onClass rdf:resource="#File"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#File">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#User">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Activity">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#InitialCommit">
  <rdfs:subClassOf rdf:resource="#Commit"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Commit"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#parent"/>
          <owl:onClass rdf:resource="#Commit"/>
          <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:maxQualifiedCardinality>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <owl:disjointWith rdf:resource="#MergeCommit"/>
</owl:Class>

<owl:Class rdf:about="#MergeCommit">
  <rdfs:subClassOf rdf:resource="#Commit"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Commit"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#parent"/>
          <owl:onClass rdf:resource="#Commit"/>
          <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>


</rdf:RDF>
