<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/git-onto-logic"
         xmlns="http://example.org/git-onto-logic#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#">

<owl:Ontology rdf:about="http://example.org/git-onto-logic"/>

<owl:ObjectProperty rdf:about="#hasBranch">
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:range rdf:resource="#Branch"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasCommit">
  <rdfs:domain rdf:resource="#Branch"/>
  <rdfs:range rdf:resource="#Commit"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#authoredBy">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#User"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#onBranch">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#Branch"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#updatesFile">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#File"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasIssue">
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:range rdf:resource="#Issue"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasPullRequest">
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:range rdf:resource="#PullRequest"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#openedBy">
  <rdfs:domain rdf:resource="#Issue"/>
  <rdfs:domain rdf:resource="#PullRequest"/>
  <rdfs:range rdf:resource="#User"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#assignedTo">
  <rdfs:domain rdf:resource="#Issue"/>
  <rdfs:range rdf:resource="#User"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#mergedBy">
  <rdfs:domain rdf:resource="#PullRequest"/>
  <rdfs:range rdf:resource="#User"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#parent">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#Commit"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasName">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#message">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#timestamp">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#isMain">
  <rdfs:domain rdf:resource="#Branch"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#status">
  <rdfs:domain rdf:resource="#Issue"/>
  <rdfs:domain rdf:resource="#PullRequest"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Repository">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasBranch"/>
      <owl:onClass rdf:resource="#Branch"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Branch">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCommit"/>
      <owl:onClass rdf:resource="#Commit"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Commit">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#authoredBy"/>
      <owl:onClass rdf:resource="#User"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#onBranch"/>
      <owl:onClass rdf:resource="#Branch"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#User">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#File">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#PullRequest">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Issue">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#MergeCommit">
  <rdfs:subClassOf rdf:resource="#Commit"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Commit"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#parent"/>
          <owl:onClass rdf:resource="#Commit"/>
          <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#InitialCommit">
  <rdfs:subClassOf rdf:resource="#Commit"/>
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Commit"/>
        <owl:Class>
          <owl:complementOf>
            <owl:Restriction>
              <owl:onProperty rdf:resource="#parent"/>
              <owl:someValuesFrom rdf:resource="#Commit"/>
            </owl:Restriction>
          </owl:complementOf>
        </owl:Class>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#SecurityCommit">
  <rdfs:subClassOf rdf:resource="#Commit"/>
</owl:Class>

<swrl:Variable rdf:about="urn:swrl#c"/>

<swrl:Variable rdf:about="urn:swrl#m"/>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#Commit"/>
          <swrl:argument1 rdf:resource="urn:swrl#c"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#message"/>
              <swrl:argument1 rdf:resource="urn:swrl#c"/>
              <swrl:argument2 rdf:resource="urn:swrl#m"/>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:SameIndividualAtom>
                  <swrl:argument1 rdf:resource="urn:swrl#m"/>
                  <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">security fix</swrl:argument2>
                </swrl:SameIndividualAtom>
              </rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#SecurityCommit"/>
          <swrl:argument1 rdf:resource="urn:swrl#c"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>


</rdf:RDF>
